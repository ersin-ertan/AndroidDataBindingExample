		android:label="@string/app_name"
		android:theme="@style/AppTheme">

		<activity
			android:name=".MainActivity"
			android:label="@string/app_name">

		</activity>

		<activity
			android:name=".practice.P01Activity"
			android:label="@string/title_activity_p01">
