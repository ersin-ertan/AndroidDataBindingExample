		<activity
			android:name=".examples.ExampleActivity"
			android:label="@string/title_activity_example">

			<intent-filter>

				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>

			</intent-filter>

		</activity>

	</application>

</manifest>
